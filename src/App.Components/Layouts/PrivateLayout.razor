@inherits MainLayout

<MudLayout>
    <MudAppBar Fixed="true" Elevation="1">
        <MudSpacer />
        <MudIconButton Icon="@Icons.Material.Filled.MoreVert" Color="Color.Inherit" Edge="Edge.End" />
    </MudAppBar>    

    <MudDrawer Fixed="true" Open="true">
        <MudDrawerHeader>
        </MudDrawerHeader>
        <MudNavMenu Bordered="true">
            <MudText Typo="Typo.h6" Class="px-4">BlazorBoilerplate</MudText>
            <MudText Typo="Typo.body2" Class="px-4 mud-text-secondary">Dashboard</MudText>
        </MudNavMenu>
    </MudDrawer>

    <MudMainContent>
        @Body
    </MudMainContent>
</MudLayout>